@using Core.Models

@model Team
<div class="container">
    <div class="row">
        <form method="post" enctype="multipart/form-data">
            <div class="mb-3">
                <label asp-for="FullName" class="form-label">FullName</label>
                <input asp-for="FullName" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
                <span asp-validation-for="FullName"></span>
            </div>
            <div class="mb-3">
                <label asp-for="Position" class="form-label">Position</label>
                <input asp-for="Position" class="form-control" id="exampleInputPassword1">
                <span asp-validation-for="Position"></span>

            </div>
            <div class="mb-3">
                <label asp-for="Description" class="form-label">Description</label>
                <input asp-for="Description" class="form-control" id="exampleInputPassword1">
                <span asp-validation-for="Description"></span>

            </div>
            <div class="mb-3">
                <label asp-for="PhotoFile" class="form-label">PhotoFile</label>
                <input asp-for="PhotoFile" class="form-control" id="exampleInputPassword1">
                <span asp-validation-for="PhotoFile"></span>

            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
</div>